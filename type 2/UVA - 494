#include<bits/stdc++.h>
using namespace std;

int main ()
{


    int x,i=0,j=0,k;

string s;

 vector<char>v;

while(getline(cin,s))
{
    int t=0;
    string l;
    int len=s.size();
for(i=0;i<len;i++)
{
    if(isalpha(s[i])==0 && isalpha(s[i-1])!=0 )
    {
        t++;

    }

}


cout<<t<<endl;





}


}

